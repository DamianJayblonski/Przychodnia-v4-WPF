<Window x:Class="Przychodnia_v4.UsunWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przychodnia_v4"
        mc:Ignorable="d"
        Title="UsunWindow" Height="200" Width="375">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,47,0,0" TextWrapping="Wrap" Text="Czy na pewno chcesz usunąć?" VerticalAlignment="Top" FontSize="15"/>
        <Button Content="Tak" HorizontalAlignment="Left" Margin="249,111,0,0" VerticalAlignment="Top" Width="57" Click="Tak_Click"/>
        <Button Content="Nie" HorizontalAlignment="Left" Margin="74,111,0,0" VerticalAlignment="Top" Width="57" Click="Nie_Click"/>

    </Grid>
</Window>
